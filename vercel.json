{
  "version": 2,
  "devCommand": "yarn dev",
  "installCommand": "yarn install",
  "buildCommand": "yarn build",
  "outputDirectory": ".",
  "public": false,
  "framework": null,
	"functions": {
	  "api/vitals/index.js": {
      "memory": 1024,
      "maxDuration": 10
    },
    "api/headers/index.js": {
      "memory": 1024,
      "maxDuration": 10
    },
    "api/db/index.ts": {
      "memory": 1024,
      "maxDuration": 10
    }
  },
	"git": {
		"deploymentEnabled": true
	},
	"github": {
		"enabled": true,
		"silent": false
	},
	"headers": [
		{
			"source": "/service-worker.js",
			"headers": [
				{
					"key": "Cache-Control",
					"value": "public, max-age=0, must-revalidate"
				}
			]
		},
		{
			"source": "/(.html)",
			"headers": [
				{
					"key": "X-UA-Compatible",
					"value": "ie-edge"
				},
				{
					"key": "Vercel-CDN-Cache-Control",
					"value": "max-age=3600"
				},
				{
					"key": "CDN-Cache-Control",
					"value": "max-age=60"
				},
				{
					"key": "Cache-Control",
					"value": "public, max-age=10, s-maxage=86400, immutable"
				},
				{
					"key": "X-XSS-Protection",
					"value": "1; mode=block"
				}
			]
		},
		{
			"source": "github-readme-stats.stoneydsp.com/(.*)",
			"headers": [
				{
					"key": "Vercel-CDN-Cache-Control",
					"value": "max-age=3600"
				},
				{
					"key": "CDN-Cache-Control",
					"value": "max-age=60"
				},
				{
					"key": "Cache-Control",
					"value": "public, max-age=10, s-maxage=86400, immutable"
				}
			]
		},
		{
			"source": "skillicons.dev/",
			"headers": [
				{
					"key": "Vercel-CDN-Cache-Control",
					"value": "max-age=3600"
				},
				{
					"key": "CDN-Cache-Control",
					"value": "max-age=60"
				},
				{
					"key": "Cache-Control",
					"value": "public, max-age=10, s-maxage=86400, immutable"
				}
			]
		},
		{
			"source": "/(.*)",
			"headers": [
				{
					"key": "X-Content-Type-Options",
					"value": "nosniff"
				},
				{
					"key": "X-Frame-Options",
					"value": "DENY"
				}
			]
		},
		{
			"source": "/:path*",
			"has": [
				{
					"type": "query",
					"key": "authorized"
				}
			],
		"headers": [
				{
					"key": "x-authorized",
					"value": "true"
				}
			]
		}
	],
	"redirects": [
		{
			"source": "/.env",
			"destination": "/api/headers?query=nope%20nothing%20there"
		},
		{
			"source": "/.git/",
			"destination": "/api/headers?query=fuck%20off%20cunt"
		},
		{
			"source": "/wp/",
			"destination": "/api/headers?query=this%20is%20not%20a%20wordpress%20site%20you%20twat"
		},
		{
			"source": "/wp/",
			"destination": "/api/headers?query=this%20is%20not%20a%20wordpress%20site%20you%20twat"
		},
		{
			"source": "/.git/config",
			"destination": "/api/headers?query=unlucky"
		},
		{
			"source": "/_all_dbs",
			"destination": "/api/headers?query=yes%20here%20is%20my%20credit%20card%20info%20%you%20dickhead"
		},
		{
			"source": "/login.action",
			"destination": "/api/headers?query=not%20giving%20me%20enough%20much%20credit%20here"
		}
	]
}
