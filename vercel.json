{
	"version": 2,
	"devCommand": "yarn dev",
	"installCommand": "yarn install",
	"buildCommand": "yarn build",
	"outputDirectory": ".",
	"public": false,
	"framework": null,
	"functions": {
		"api/vitals/index.js": {
			"memory": 1024,
			"maxDuration": 10
		},
		"api/headers/index.js": {
			"memory": 1024,
			"maxDuration": 10
		},
		"api/db/index.ts": {
			"memory": 1024,
			"maxDuration": 10
		}
	},
	"headers": [
		{
			"source": "/(.html)",
			"headers": [
				{
					"key": "X-UA-Compatible",
					"value": "ie-edge"
				},
				{
					"key": "Vercel-CDN-Cache-Control",
					"value": "max-age=3600"
				},
				{
					"key": "CDN-Cache-Control",
					"value": "max-age=60"
				},
				{
					"key": "Cache-Control",
					"value": "public, max-age=10, s-maxage=86400, immutable"
				},
				{
					"key": "X-XSS-Protection",
					"value": "1; mode=block"
				}
			]
		},
		{
			"source": "github-readme-stats.stoneydsp.com/(.*)",
			"headers": [
				{
					"key": "Vercel-CDN-Cache-Control",
					"value": "max-age=3600"
				},
				{
					"key": "CDN-Cache-Control",
					"value": "max-age=60"
				},
				{
					"key": "Cache-Control",
					"value": "public, max-age=10, s-maxage=86400, immutable"
				}
			]
		},
		{
			"source": "skillicons.dev/",
			"headers": [
				{
					"key": "Vercel-CDN-Cache-Control",
					"value": "max-age=3600"
				},
				{
					"key": "CDN-Cache-Control",
					"value": "max-age=60"
				},
				{
					"key": "Cache-Control",
					"value": "public, max-age=10, s-maxage=86400, immutable"
				}
			]
		},
		{
			"source": "/(.*)",
			"headers": [
				{
					"key": "X-Content-Type-Options",
					"value": "nosniff"
				},
				{
					"key": "X-Frame-Options",
					"value": "DENY"
				}
			]
		},
		{
			"source": "/public/(.*)",
			"headers": [
				{
					"key": "Access-Control-Allow-Origin",
					"value": "*"
				}
			]
		},
		{
			"source": "/:path*",
			"has": [
				{
					"type": "query",
					"key": "authorized"
				}
			],
		"headers": [
				{
					"key": "x-authorized",
					"value": "true"
				}
			]
		}
	],
	"redirects": [
    {
      "source": "/.env",
			"destination": "/404.html"
		},
		{
			"source": "/.git/",
			"destination": "/404.html"
		},
		{
			"source": "/wp/",
			"destination": "/404.html"
		},
		{
			"source": "/.git/config",
			"destination": "/404.html"
		},
		{
			"source": "/_all_dbs",
			"destination": "/404.html"
		},
		{
			"source": "/login.action",
			"destination": "/404.html"
		},
		{
